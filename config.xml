    <allow-navigation href="http://192.168.1.2:8100" />
    <allow-navigation href="http://192.168.1.3:8100" />
    <allow-navigation href="http://192.168.3.159:8100" />
    <plugin name="cordova-plugin-ionic-keyboard" spec="^2.1.2" />
    <plugin name="cordova-plugin-ionic-webview" spec="^1.2.1" />
    <plugin name="cordova-plugin-statusbar" spec="^2.4.2" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="cordova-plugin-facebook4" spec="^2.2.0">
        <variable name="APP_ID" value="214250519334220" />
        <variable name="APP_NAME" value="SOS Method App" />
        <variable name="ANDROID_SDK_VERSION" value="4.+" />
    </plugin>
    <plugin name="cc.fovea.cordova.purchase" spec="^7.1.3">
        <variable name="BILLING_KEY" value="" />
    </plugin>
    <plugin name="cordova-android-support-gradle-release" spec="~1.4.2" />
    <plugin name="cordova-plugin-splashscreen" spec="^5.0.2" />
    <plugin name="cordova-plugin-is-debug" spec="^1.0.0" />
    <plugin name="cordova-plugin-file" spec="^6.0.1" />
