        <variable name="SENDER_ID" value="653273040369" />
        <variable name="653273040369" value="11.6.3" />
    </plugin>
    <plugin name="cordova-plugin-market" spec="^1.2.0" />
    <plugin name="ionic-plugin-deeplinks" spec="^1.0.17">
        <variable name="URL_SCHEME" value="sosmethodapp" />
        <variable name="DEEPLINK_SCHEME" value="https" />
        <variable name="DEEPLINK_HOST" value="sosmethod.com" />
    </plugin>
    <plugin name="cordova-plugin-device" spec="^2.0.2" />
    <plugin name="cordova-plugin-device-name" spec="^1.3.4" />
    <engine name="ios" spec="~4.5.5" />
</widget>
